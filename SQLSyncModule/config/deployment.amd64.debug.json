{
  "modulesContent": {
    "$edgeAgent": {
      "properties.desired": {
        "schemaVersion": "1.1",
        "runtime": {
          "type": "docker",
          "settings": {
            "minDockerVersion": "v1.25",
            "loggingOptions": "",
            "registryCredentials": {}
          }
        },
        "systemModules": {
          "edgeAgent": {
            "type": "docker",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-agent:1.2",
              "createOptions": "{}"
            }
          },
          "edgeHub": {
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-hub:1.2",
              "createOptions": "{\"HostConfig\":{\"PortBindings\":{\"5671/tcp\":[{\"HostPort\":\"5671\"}],\"8883/tcp\":[{\"HostPort\":\"8883\"}],\"443/tcp\":[{\"HostPort\":\"443\"}]}}}"
            }
          }
        },
        "modules": {
          "SQLSyncModule2": {
            "version": "1.0.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "avasampleregistryebkkuonvyhsna.azurecr.io/sqlsyncmodule:0.0.1-amd64.debug",
              "createOptions": "{}"
            }
          },
          "FakeTicketGenerator": {
            "version": "1.0.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "avasampleregistryebkkuonvyhsna.azurecr.io/faketicketgenerator:0.0.1-amd64.debug",
              "createOptions": "{}"
            }
          },
          "SQLSyncModule": {
            "version": "1.0.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "avasampleregistryebkkuonvyhsna.azurecr.io/sqlsyncmodule:0.0.1-amd64.debug",
              "createOptions": "{}"
            }
          }
        }
      }
    },
    "$edgeHub": {
      "properties.desired": {
        "schemaVersion": "1.2",
        "routes": {
          "SQLSyncModule2ToIoTHub": "FROM /messages/modules/SQLSyncModule2/outputs/* INTO $upstream",
          "FakeTicketGeneratorToIoTHub": "FROM /messages/modules/FakeTicketGenerator/outputs/* INTO $upstream",
          "SQLSyncModuleToIoTHub": "FROM /messages/modules/SQLSyncModule/outputs/* INTO $upstream"
        },
        "storeAndForwardConfiguration": {
          "timeToLiveSecs": 7200
        }
      }
    }
  }
}